<?xml version="1.0"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <parent>
        <artifactId>eSport</artifactId>
        <groupId>ejava.javaee.projects</groupId>
        <version>1.0.2007.2</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <groupId>ejava.javaee.projects.eSport</groupId>
    <artifactId>eLeague</artifactId>
    <packaging>pom</packaging>
    <name>eLeague</name>
    <version>1.0.2007.2</version>
    <description>
        This project provides test data and sample code to be used in the
        eLeague project.
    </description>

    <profiles>
        <profile>
            <id>devel</id>
            <activation>
                <property>
                    <name>devel</name>
                    <value>true</value>
                </property>
            </activation>
            <modules>
                <module>eLeagueDTO</module>
                <module>eLeagueDataGen</module>
                <module>eLeagueTestUtil</module>
            </modules>
        </profile>
        <profile>
            <id>deploy</id>
            <activation>
                <property>
                    <name>!devel</name>
                </property>
            </activation>
            <modules>
                <module>eLeagueDTO</module>
                <module>eLeagueTestUtil</module>
                <module>eLeagueBO</module>
                <module>eLeagueDAO</module>
                <module>eLeagueBLImpl</module>
            </modules>
        </profile>
    </profiles>
</project>
